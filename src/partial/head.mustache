<head>
  <meta charset="{{encoding}}">

  <title>{{^isHome}}{{shortTitle}} - {{/isHome}}{{siteTitle}}</title>

  <meta content="{{description}}" name="description">
  <meta content="hsl(220, 100%, 40%)" name="theme-color">
  <meta content="width=device-width" name="viewport">

  <link href="/apple-touch-icon-precomposed.png" rel="apple-touch-icon">
  <link href="{{canonical}}" rel="canonical">
  {{#isHome}}{{#items}}{{#isLatest}}<link href="{{link}}" rel="prerender">{{/isLatest}}{{/items}}{{/isHome}}
  <link href="https://fonts.googleapis.com/css?family=Noto+Sans+JP:400,700" rel="stylesheet">

  <style>{{> css}}</style>

  <script async defer src="/js/main.min.js"></script>
  <script>{{> js}}</script>
</head>
