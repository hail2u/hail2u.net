{{> header}}

{{#statuses}}
	{{#isFirstInDate}}
		<section>
			<h2><time datetime="{{strYear}}-{{strMonth}}-{{strDate}}">{{year}}年{{month}}月{{date}}日</time></h2>

			<ol class="posts">
	{{/isFirstInDate}}

				<li id="on-{{published}}">
					<p class="metaline"><time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{hour}}時{{minute}}分</time></p>
					<p>{{text}}</p>
				</li>

	{{#isLastInDate}}
			</ol>
		</section>
	{{/isLastInDate}}
{{/statuses}}

<hr>

<nav>
	<p><a href="{{canonical}}log.html"><span aria-hidden="true">→</span>Twitterでのツイート（～2018年12月30日）</a></p>
</nav>

{{> footer}}
