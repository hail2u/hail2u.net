<!DOCTYPE html>
<html lang="{{lang}}">
  {{> head}}

  <body>
    {{> header}}

    <main>
      {{#texts}}
        <section id="on-{{published}}">
          <h1><time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{strYear}}/{{strMonth}}/{{strDate}} {{strHour}}:{{strMinute}}</time></h1>

          <p>{{text}}</p>
        </section>
      {{/texts}}
    </main>

    {{> footer}}
  </body>
</html>
