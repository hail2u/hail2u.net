<meta content="{{description}}" itemprop="description">
<meta content="{{image}}" itemprop="image">
<link href="{{canonical}}" itemprop="url">

{{#home}}<meta content="{{site_title}}" property="og:title">{{/home}}
{{^home}}<meta content="{{short_title}} - {{site_title}}" property="og:title">{{/home}}
<meta content="{{type}}" property="og:type">
