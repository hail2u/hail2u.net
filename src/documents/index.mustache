<!DOCTYPE html>
<html lang="{{lang}}">
	{{> head}}

	<body>
		{{> page-header}}

		{{#documents}}
			<section>
				<header>
					<p class="date"><time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{year}}年{{month}}月{{date}}日</time></p>

					<h2><a href="{{link}}">{{title}}</a></h2>
				</header>

				<p>{{description}}</p>
			</section>
		{{/documents}}

		{{> page-footer}}
	</body>
</html>
