<?xml version="1.0" encoding="{{encoding}}"?>
<rss version="2.0"
	xml:lang="{{lang}}"
	xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:creativeCommons="http://backend.userland.com/creativeCommonsRssModule">
	<channel>
		<description>{{description}}</description>
		<link>{{scheme}}://{{domain}}{{canonical}}</link>
		<managingEditor>{{email}} ({{fullname}})</managingEditor>
		<title>{{^isHome}}{{shortTitle}} - {{/isHome}}{{siteTitle}}</title>
		<atom:link href="https://pubsubhubbub.appspot.com/" rel="hub"/>
		<atom:link href="{{scheme}}://{{domain}}{{feedURL}}" rel="self"/>
		<creativeCommons:license>{{licenseURL}}</creativeCommons:license>
		{{#items}}
			<item>
				<description><![CDATA[{{description}}]]></description>
				<link>{{link}}</link>
				<pubDate>{{strDowName}}, {{date}} {{strMonthName}} {{strYear}} {{strHour}}:{{strMinute}}:{{strSecond}} +0900</pubDate>
				<title><![CDATA[{{title}}]]></title>
				{{#body}}<content:encoded><![CDATA[{{{body}}}]]></content:encoded>{{/body}}
			</item>
		{{/items}}
	</channel>
</rss>
