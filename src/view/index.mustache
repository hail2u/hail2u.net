<!DOCTYPE html>
<html lang="{{lang}}">
  {{> head}}

  <body>
    {{> header}}

    <main>
      <section>
        <h1>{{short_title}}</h1>

        <ol role="directory">
          {{#links}}
            <li>
              <time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{year}}年{{month}}月{{date}}日</time>
              <a href="{{url}}">{{title}}</a>
            </li>
          {{/links}}
        </ol>
      </section>
    </main>

    {{> footer}}
  </body>
</html>
