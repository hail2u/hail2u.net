<!DOCTYPE html>
<html lang="{{lang}}">
	{{> head}}

	<body>
		{{> page-header}}

		{{#documents}}
			<p><time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{strYear}}-{{strMonth}}-{{strDate}}</time>&#x2003;&#x2003;<a href="{{link}}">{{title}}</a></p>
		{{/documents}}

		<hr>

		{{#articles}}
			<p><time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{strYear}}-{{strMonth}}-{{strDate}}</time>&#x2003;&#x2003;<a href="{{link}}">{{title}}</a></p>
		{{/articles}}

		<hr>

		<ol class="showcase">
			{{#books}}
				<li>{{{body}}}</li>
			{{/books}}
		</ol>

		<hr>

		{{#links}}
			<p><time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{strYear}}-{{strMonth}}-{{strDate}}</time>&#x2003;&#x2003;<a href="{{link}}">{{title}}</a></p>
		{{/links}}

		{{> page-footer}}
	</body>
</html>
