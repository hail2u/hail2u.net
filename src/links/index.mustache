<!DOCTYPE html>
<html lang="{{lang}}" itemscope itemtype="http://schema.org/{{schemaType}}">
  {{> head}}

  <body>
    {{> header}}

    <main>
      {{#links}}
        {{#isFirstInMonth}}
          <section id="on-{{strYear}}-{{strMonth}}">
            <h2><time datetime="{{strYear}}-{{strMonth}}">{{year}}年{{month}}月</time></h2>

            <ol>
        {{/isFirstInMonth}}

        <li>
          <time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{date}}日</time>
          <a href="{{url}}">{{title}}</a>
        </li>

        {{#isLastInMonth}}
            </ol>
          </section>
        {{/isLastInMonth}}
      {{/links}}
    </main>

    {{> footer}}
  </body>
</html>
