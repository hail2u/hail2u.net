<!DOCTYPE html>
<html lang="{{lang}}">
	{{> head}}

	<body>
		{{> page-header}}

		{{#links}}
			<p class="date"><time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{year}}年{{month}}月{{date}}日</time></p>

			<p><a href="{{link}}">{{title}}</a>: {{description}}</p>
		{{/links}}

		{{> page-footer}}
	</body>
</html>
