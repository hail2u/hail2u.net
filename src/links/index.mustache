<!DOCTYPE html>
<html lang="{{lang}}">
  {{> head}}

  <body>
    {{> header}}

    <main>
      <section>
        <h1>ウェブページ</h1>

        <ol>
          {{#links}}
            <li>
              <time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{year}}年{{month}}月{{date}}日</time>
              <a href="{{url}}">{{title}}</a>
            </li>
          {{/links}}
        </ol>
      </section>

      <nav>
        <p><a href="{{canonical}}log.html">{{shortTitle}}の全記録</a></p>
      </nav>
    </main>

    {{> footer}}
  </body>
</html>
