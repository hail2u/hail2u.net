<!DOCTYPE html>
<html lang="{{lang}}">
  {{> head}}

  <body>
    {{> page-header}}

    {{#statuses}}
      {{#isFirstInMonth}}
        <section{{#isFirstInYear}} id="{{strYear}}"{{/isFirstInYear}}>
          <h2><time datetime="{{strYear}}-{{strMonth}}">{{year}}年{{month}}月</time></h2>
      {{/isFirstInMonth}}

      <p id="{{published}}">{{title}} (<a href="{{link}}"><time datetime="{{strYear}}-{{strMonth}}-{{strDate}}T{{strHour}}:{{strMinute}}:{{strSecond}}+09:00">{{month}}月{{date}}日{{hour}}時{{minute}}分</time></a>)</p>

      {{#isLastInMonth}}
        </section>
      {{/isLastInMonth}}
    {{/statuses}}

    {{{log}}}

    {{> page-footer}}
  </body>
</html>
