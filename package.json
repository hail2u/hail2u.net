{
  "name": "hail2u.net",
  "description": "Here is a decent document, a simple translation, and a weblog article about web development. These are written by Kyo Nagashima.",
  "version": "6.112.4",
  "author": "Kyo Nagashima <hail2u@gmail.com> (https://kyonagashima.com/)",
  "bugs": {
    "url": "https://github.com/hail2u/hail2u.net/issues"
  },
  "devDependencies": {
    "css-mqpacker": "^5.0.1",
    "csswring": "^5.0.0",
    "feedmix": "^1.0.0",
    "fs-extra": "^2.0.0",
    "google-closure-compiler-js": "^20170218.0.0",
    "html-minifier": "^3.3.0",
    "http-server": "^0.9.0",
    "marked": "^0.3.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mustache": "^2.2.1",
    "pit-ro": "^0.0.1",
    "postcss": "^5.2.11",
    "postcss-round-float": "^0.1.0",
    "request": "^2.80.0",
    "which": "^1.0.8",
    "xml2js": "^0.4.1"
  },
  "homepage": "https://hail2u.net/",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/hail2u/hail2u.net.git"
  },
  "scripts": {
    "articles": "node ./bin/articles.js",
    "postarticles": "npm run html",
    "blog": "node ./bin/blog.js",
    "postblog": "git add -- ./src/weblog/plugins/state/ && git diff --cached --exit-code --quiet || git commit --message=\"Update cache files\"",
    "clear": "rm -fr ./tmp/ && mkdir tmp",
    "precss": "npm run test_css && npm run clear",
    "css": "node ./bin/css.js",
    "postcss": "cp -rT ./tmp/ ./dist/styles/ && npm run style_guide",
    "deploy": "cd ./dist/ && git add --all && git commit --message=\"Build\" && git push origin gh-pages",
    "html": "node ./bin/html.js",
    "posthtml": "npm run test_html && npm run index",
    "preimg": "npm run clear",
    "img": "node ./bin/png.js && node ./bin/favicon.js",
    "postimg": "cp -rT ./src/img/ ./dist/images/",
    "index": "node ./bin/feed.js && node ./bin/sitemap.js",
    "prejs": "npm run test_js && npm run clear",
    "js": "node ./bin/js.js",
    "postjs": "cp -rT ./src/js/ ./dist/scripts/ && cp -rT ./tmp/ ./dist/scripts/",
    "prepost": "npm run clear",
    "post": "node ./bin/simplenote.js --publish",
    "prepreview": "npm run clear",
    "preview": "node ./bin/simplenote.js",
    "public": "node ./bin/public.js",
    "postpublic": "cd ./dist/ && git commit --message=\"Upload\" && git push origin gh-pages",
    "start": "http-server ./dist/ -o",
    "style_guide": "node ./bin/style-guide.js",
    "test": "npm run test_html && npm run test_css && npm run test_js",
    "test_css": "stylelint --syntax scss \"./src/css/**/*.scss\"",
    "test_html": "htmlhint ./src/css/test.html \"./dist/**/index.html\" \"./dist/documents/**/*.html\"",
    "test_html_arg": "node ./bin/test_html_arg.js",
    "test_js": "eslint ./bin/ ./lib/ ./src/js/",
    "preupdate": "npm run clear",
    "update": "node ./bin/update.js"
  }
}
